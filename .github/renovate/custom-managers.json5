{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  customManagers: [
    {
      customType: "regex",
      description: "Process common lib json schema references",
      fileMatch: ["\\.json$"],
      matchStrings: [
        "https:\\/\\/raw.githubusercontent.com\\/owner\\/helm-charts\\/(?<depName>\\S+?)-(?<currentValue>[\\d\\.]+?)\\/\\S+",
      ],
      datasourceTemplate: "helm",
      versioningTemplate: "semver",
      registryUrlTemplate: "https://owner.github.io/helm-charts"
    },
  ],
}
