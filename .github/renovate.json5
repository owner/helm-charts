{
  extends: [
    "github>bjw-s/renovate-config",
    "github>bjw-s/renovate-config:automerge-github-actions",
    "github>owner/helm-charts//.github/renovate/custom-managers.json5",
  ],
  packageRules: [
    {
      description: "Auto merge minors and patches for mise packages",
      matchManagers: ["mise"],
      automerge: true,
      automergeType: "pr",
      matchUpdateTypes: ["minor", "patch"],
      ignoreTests: false,
    },
  ],
}
